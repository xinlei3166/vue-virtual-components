    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue-virtual-components/antd-table": "workspace:*",
    "@vue-virtual-components/utils": "workspace:*",
    "@yunquejs/fabric": "^0.1.5",
    "@yunquejs/release": "^0.1.9",
    "consola": "^3.2.3",
